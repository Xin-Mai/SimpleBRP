<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>
<VBox fx:controller="sample.Controller"
      maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="1067.0" spacing="10.0" stylesheets="@mainStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox maxHeight="60.0" prefHeight="60.0" prefWidth="1067.0" spacing="10.0" styleClass="bg" VBox.vgrow="NEVER">
            <children>
                <Button fx:id="check" text="查看订单"
                        onAction="#handleCheckButtonAction"
                        maxWidth="Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="160.0"  HBox.hgrow="ALWAYS">
                </Button>
                <Button fx:id="manage" text="订单管理"
                        onAction="#handleManageButtonAction"
                        maxWidth="Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="160.0"  HBox.hgrow="ALWAYS">
                </Button>
                <Button fx:id="add" text="新建订单"
                        onAction="#handleAddButtonAction"
                        maxWidth="Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="160.0"  HBox.hgrow="ALWAYS">
                </Button>
                <Button fx:id="summaryData" text="综合数据"
                        onAction="#handleDataButtonAction"
                        maxWidth="Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="160.0"  HBox.hgrow="ALWAYS">
                </Button>
                <HBox maxHeight="60.0" prefHeight="60.0" prefWidth="400.0" spacing="10.0" styleClass="bg" alignment="TOP_CENTER" HBox.hgrow="ALWAYS">
                    <children>
                        <TextField prefHeight="40.0" prefWidth="300.0" promptText="查询" HBox.hgrow="ALWAYS"/>
                        <Button fx:id="search" text="搜索"
                                alignment="TOP_CENTER"
                                onAction="#handleSearchButtonAction"
                                maxWidth="Infinity" mnemonicParsing="false" prefHeight="40.0" prefWidth="60.0" HBox.hgrow="SOMETIMES">
                        </Button>
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
                    </padding>
                </HBox>

            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding></HBox>
    </children>
    <padding>
        <Insets top="10.0" />
    </padding>
    <FlowPane fx:id="pane">
    </FlowPane>
</VBox>

